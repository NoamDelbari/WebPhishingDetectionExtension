    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\simple_test.js"
        }
        ,
        {
            "type": "python",
            "request": "launch",
            "name": "Run train.py",
            "program": "${workspaceFolder}/phishing_detector/training/train.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env":{
                "PYTHONPATH": "${workspaceFolder}"
            },
        },
        {
            "type": "python",
            "request": "launch",
            "name": "Run predict.py",
            "program": "${workspaceFolder}/phishing_detector/evaluation/predict.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env":{
                "PYTHONPATH": "${workspaceFolder}"
            },
        }
    ]
}